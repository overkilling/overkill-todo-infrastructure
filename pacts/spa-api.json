{
  "consumer": {
    "name": "SPA"
  },
  "provider": {
    "name": "API"
  },
  "interactions": [
    {
      "description": "healthcheck",
      "providerState": "application is healthy",
      "request": {
        "method": "GET",
        "path": "/health"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "status": "ok"
        },
        "matchingRules": {
          "$.body.status": {
            "match": "regex",
            "regex": "ok|fail"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}